        "getOption": "const data = context.panel.data.series[0];\nif (!data || !data.fields || data.fields.length === 0) {\n  return {};\n}\n\nconst hostnameField = data.fields.find(f => f.name === 'hostname');\nconst minField = data.fields.find(f => f.name === 'min');\nconst q1Field = data.fields.find(f => f.name === 'q1');\nconst medianField = data.fields.find(f => f.name === 'median');\nconst q3Field = data.fields.find(f => f.name === 'q3');\nconst maxField = data.fields.find(f => f.name === 'max');\n\nif (!hostnameField || !minField || !q1Field || !medianField || !q3Field || !maxField) {\n  return {};\n}\n\nconst categories = hostnameField.values.toArray();\nconst boxplotData = [];\n\nfor (let i = 0; i < categories.length; i++) {\n  boxplotData.push([\n    minField.values.get(i),\n    q1Field.values.get(i),\n    medianField.values.get(i),\n    q3Field.values.get(i),\n    maxField.values.get(i)\n  ]);\n}\n\nreturn {\n  tooltip: {\n    trigger: 'item',\n    axisPointer: {\n      type: 'shadow'\n    }\n  },\n  grid: {\n    left: '10%',\n    right: '10%',\n    bottom: '15%',\n    top: '10%'\n  },\n  xAxis: {\n    type: 'category',\n    data: categories,\n    name: 'Device',\n    nameLocation: 'middle',\n    nameGap: 30,\n    axisLabel: {\n      rotate: 45\n    }\n  },\n  yAxis: {\n    type: 'value',\n    name: 'Power (W)',\n    nameLocation: 'middle',\n    nameGap: 50,\n    scale: true\n  },\n  series: [\n    {\n      name: 'boxplot',\n      type: 'boxplot',\n      data: boxplotData,\n      itemStyle: {\n        color: '#5470c6',\n        borderColor: '#5470c6'\n      }\n    }\n  ]\n};",
