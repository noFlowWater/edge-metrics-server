        "visualEditor": {
          "code": "const data = context.panel.data.series[0];\n\nif (!data || !data.fields || data.fields.length === 0) {\n  return {};\n}\n\n// Ï†ÑÏ≤¥ ÌïÑÎìú Ïù¥Î¶Ñ Ìïú Î≤à Î™®ÏïÑÏÑú Î≥¥Ïûê (ÎîîÎ≤ÑÍπÖÏö©)\nconst fieldNames = data.fields.map(f => f.name);\n\n// üîß Ï†ïÎ†¨ Í∏∞Ï§Ä: 'median' ÎòêÎäî 'mean'\nconst SORT_BY = 'mean';\n\n// ÌïÑÎìú Ï∞æÍ∏∞ (ÌïÑÏöîÌïòÎ©¥ ÏõêÎûò Ïù¥Î¶Ñ + Value #X Îëò Îã§ ÌóàÏö©)\nconst hostnameField = data.fields.find(f => f.name === 'hostname');\nconst minField = data.fields.find(f => f.name === 'min' || f.name === 'Value #A');\nconst q1Field = data.fields.find(f => f.name === 'q1' || f.name === 'Value #B');\nconst medianField = data.fields.find(f => f.name === 'median' || f.name === 'Value #C');\nconst q3Field = data.fields.find(f => f.name === 'q3' || f.name === 'Value #D');\nconst maxField = data.fields.find(f => f.name === 'max' || f.name === 'Value #E');\n// ‚≠ê mean ÏùÄ renameÏù¥ Ïïà ÎêòÏóàÏùÑ ÏàòÎèÑ ÏûàÏúºÎãà Îëò Îã§ Ï≤¥ÌÅ¨\nconst meanField = data.fields.find(f => f.name === 'mean' || f.name === 'Value #F');\n\nif (!hostnameField || !minField || !q1Field || !medianField || !q3Field || !maxField || !meanField) {\n  // ÏùºÎã® Î≠êÍ∞Ä ÏûàÎäîÏßÄ Ìå®ÎÑê Ï†úÎ™©ÏúºÎ°ú Ï∞çÏñ¥ÏÑú ÌôïÏù∏\n  return {\n    title: {\n      text: 'Missing fields. Found: ' + fieldNames.join(', ')\n    }\n  };\n}\n\n// ÎîîÎ∞îÏù¥Ïä§Î≥Ñ Îç∞Ïù¥ÌÑ∞ ÏàòÏßë\nconst deviceData = [];\nconst len = hostnameField.values.length;\n\nfor (let i = 0; i < len; i++) {\n  const hostname = hostnameField.values.get ? hostnameField.values.get(i) : hostnameField.values[i];\n  const min = Number(minField.values.get ? minField.values.get(i) : minField.values[i]);\n  const q1 = Number(q1Field.values.get ? q1Field.values.get(i) : q1Field.values[i]);\n  const median = Number(medianField.values.get ? medianField.values.get(i) : medianField.values[i]);\n  const q3 = Number(q3Field.values.get ? q3Field.values.get(i) : q3Field.values[i]);\n  const max = Number(maxField.values.get ? maxField.values.get(i) : maxField.values[i]);\n  const mean = Number(meanField.values.get ? meanField.values.get(i) : meanField.values[i]);\n\n  deviceData.push({ hostname, min, q1, median, q3, max, mean });\n}\n\n// Ï†ïÎ†¨\ndeviceData.sort((a, b) => b[SORT_BY] - a[SORT_BY]);\n\nconst categories = deviceData.map(d => d.hostname);\nconst boxplotData = deviceData.map(d => [d.min, d.q1, d.median, d.q3, d.max]);\nconst meanScatterData = deviceData.map(d => [d.hostname, d.mean]);\n\nreturn {\n  tooltip: {\n    trigger: 'item',\n    formatter: function (param) {\n      const d = deviceData[param.dataIndex];\n\n      if (param.seriesType === 'boxplot') {\n        return [\n          'Device: ' + d.hostname,\n          'Max: ' + d.max.toFixed(2) + ' W',\n          'Q3: ' + d.q3.toFixed(2) + ' W',\n          'Median: ' + d.median.toFixed(2) + ' W',\n          'Q1: ' + d.q1.toFixed(2) + ' W',\n          'Min: ' + d.min.toFixed(2) + ' W',\n          'Mean: ' + d.mean.toFixed(2) + ' W'\n        ].join('<br/>');\n      }\n\n      if (param.seriesName === 'Mean') {\n        return [\n          'Device: ' + d.hostname,\n          'Mean: ' + d.mean.toFixed(2) + ' W'\n        ].join('<br/>');\n      }\n\n      return '';\n    }\n  },\n  grid: {\n    left: '10%',\n    right: '10%',\n    bottom: '15%',\n    top: '10%'\n  },\n  xAxis: {\n    type: 'category',\n    data: categories,\n    name: 'Device',\n    nameLocation: 'middle',\n    nameGap: 40,\n    axisLabel: {\n      rotate: 0,\n      fontSize: 13,\n      margin: 10,\n      padding: [0, 0, 0, 0]\n    }\n  },\n  yAxis: {\n    type: 'value',\n    name: 'Power (W)',\n    nameLocation: 'middle',\n    nameGap: 50,\n    scale: true\n  },\n  series: [\n    {\n      name: 'Power',\n      type: 'boxplot',\n      data: boxplotData,\n      itemStyle: {\n        color: '#91CBFF',\n        borderColor: '#5FA8E8'\n      }\n    },\n    {\n      name: 'Mean',\n      type: 'scatter',\n      data: meanScatterData,\n      symbolSize: 6,\n      itemStyle: {\n        color: '#d62728'\n      }\n    }\n  ]\n};\n",
          "dataset": [],
          "series": []
        }
